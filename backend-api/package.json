{
  "name": "one-login-async-credentials",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "deploy:dev": "cd ../helper-scripts && ./deploy_backend.sh",
    "build:infra": "./combine_template.sh",
    "generate-proxy-open-api": "node ./generate-proxy-private-openapi.js",
    "format": "./node_modules/.bin/prettier --write '**/*.{ts,md}'",
    "format:check": "./node_modules/.bin/prettier --check '**/*.{ts,md}'",
    "lint": "./node_modules/.bin/eslint ./src/functions",
    "test": "./node_modules/.bin/jest --testPathIgnorePatterns=api-tests/",
    "test:api": "NODE_OPTIONS=--experimental-vm-modules ./node_modules/.bin/jest --testPathPatterns=api-tests/",
    "test:infra": "./node_modules/.bin/jest --testPathPatterns=infra-tests/",
    "test:pact": "./node_modules/.bin/jest --testPathPatterns=src/functions/tests/pact/",
    "test:pact:authorize": "bash -c 'curl -sS -o /dev/null --fail -u $PACT_BROKER_USERNAME:$PACT_BROKER_PASSWORD \"$PACT_BROKER_URL?testSource=$PACT_BROKER_SOURCE_SECRET\"'",
    "test:pact:ci": "npm run test:pact:authorize && npm run test:pact",
    "test:pact:local": ". ./src/functions/tests/pact/setPactCredentials/set_pact_credentials.sh && npm run test:pact",
    "test:unit": "./node_modules/.bin/jest --detectOpenHandles --testPathIgnorePatterns=tests/pact api-tests"
  },
  "engines": {
    "node": "22"
  },
  "devDependencies": {
    "@aws-sdk/credential-providers": "3.996.0",
    "@eslint/js": "10.0.1",
    "@pact-foundation/pact": "16.2.0",
    "@types/aws-lambda": "8.10.160",
    "@types/express": "5.0.6",
    "@types/jest": "30.0.0",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.19.11",
    "@types/node-jose": "1.1.13",
    "aws-cdk-lib": "2.240.0",
    "aws-sdk-client-mock": "4.1.0",
    "aws-sdk-client-mock-jest": "4.1.0",
    "aws4-axios": "3.4.0",
    "axios-retry": "4.5.0",
    "dotenv": "17.3.1",
    "eslint": "10.0.2",
    "express": "5.2.1",
    "jest": "30.2.0",
    "jest-junit": "16.0.0",
    "js-yaml": "4.1.1",
    "prettier": "3.8.1",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1",
    "yaml": "2.8.2",
    "yaml-cfn": "0.3.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.31.0",
    "@aws-lambda-powertools/parameters": "2.31.0",
    "@aws-sdk/client-dynamodb": "3.996.0",
    "@aws-sdk/client-kms": "3.996.0",
    "@aws-sdk/client-s3": "3.996.0",
    "@aws-sdk/client-secrets-manager": "3.996.0",
    "@aws-sdk/client-sqs": "3.996.0",
    "@aws-sdk/client-ssm": "3.996.0",
    "@aws-sdk/node-http-handler": "3.374.0",
    "@aws-sdk/util-dynamodb": "3.996.0",
    "@govuk-one-login/mobile-id-check-biometric-credential": "3.1.0",
    "axios": "1.13.5",
    "ecdsa-sig-formatter": "1.0.11",
    "esbuild": "0.27.3",
    "jose": "5.10.0"
  },
  "overrides": {
    "micromatch": "4.0.8"
  }
}
