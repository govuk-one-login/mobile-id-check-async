{
  "name": "helper-scripts",
  "version": "1.0.0",
  "description": "ID Check helper scripts",
  "type": "module",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "generateHashedSecret": "npm run build --silent && node dist/generateHashedSecret/generateHashedSecret --silent",
    "deleteStacks": "npm run build && node dist/deleteStacks/deleteStacks",
    "format": "./node_modules/.bin/prettier --write '**/*.{ts,md}'",
    "format:check": "./node_modules/.bin/prettier --check '**/*.{ts,md}'",
    "lint": "./node_modules/.bin/eslint ./src"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.996.0",
    "inquirer": "13.3.0",
    "zx": "8.8.5"
  },
  "devDependencies": {
    "@types/node": "22.19.11",
    "eslint": "10.0.2",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1"
  }
}
